{
  "name": "Monitoring-app",
  "description": "An app for viewing monitoring data",
  "version": "1.0.0-alpha1",
  "main": "src/index.js",
  "browserify": {
    "transform": [
      "babelify",
      "browserify-css",
      "brfs"
    ]
  },
  "dependencies": {
    "base64-url": "^1.3.3",
    "case": "^1.4.2",
    "debug": "^2.2.0",
    "deep-equal": "^1.0.1",
    "drag-drop": "^2.11.0",
    "feature-filter": "^2.2.0",
