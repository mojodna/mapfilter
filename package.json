    "round-to": "^1.1.0",
    "turf-extent": "^1.0.4",
    "url-regex": "^3.2.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.0",
    "browserify-hmr": "gmaclennan/browserify-hmr#gmaclennan-patch-1",
    "budo": "^8.0.4",
    "copyfiles": "^0.2.1",
    "dependency-check": "^2.5.1",
    "envify": "^3.4.1",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "gh-pages": "^0.11.0",
    "mkdirp": "^0.5.1",
    "spritesmith-cli": "^1.1.2",
    "spritezero-cli": "^1.0.2",
    "standard": "^8.5.0",
    "tape": "^4.4.0",
    "uglify-js": "^2.7.4",
    "uglifyify": "^3.0.4"
  },
  "license": "MIT",
  "scripts": {
    "dependency-check": "dependency-check ./package.json && dependency-check ./package.json --unused --no-dev",
    "lint": "standard",
    "start": "budo src/index.js:bundle.js --pushstate --dir statics",
    "dev": "budo src/dev.js:bundle.js --pushstate --dir statics -- -p browserify-hmr",
    "test": "exit 0",
    "build": "npm run build:statics && npm run build:js",
    "build:statics": "copyfiles -u 1 'statics/**/*' build",
    "build:js": "NODE_ENV=production browserify -g envify -g uglifyify src/index.js | uglifyjs -c -m > build/bundle.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build/**"
    ]
  }
}
