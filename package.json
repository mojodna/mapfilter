    "flat": "^2.0.1",
    "geojsonhint": "^1.2.0",
    "get-scrollbar-width": "^1.0.4",
    "lodash": "^4.16.4",
    "mapbox-gl": "^0.27.0",
    "material-ui": "^0.16.4",
    "moment": "^2.15.2",
    "object-assign": "^4.1.0",
    "randombytes": "^2.0.3",
    "react": "^15.4.0",
    "react-addons-shallow-compare": "^15.3.2",
    "react-date-range": "^0.2.4",
    "react-dom": "^15.4.0",
    "react-imageloader": "^2.1.0",
    "react-intl": "^2.1.5",
    "react-intl-redux": "^0.1.0",
    "react-modal": "^1.5.2",
    "react-motion": "^0.4.5",
    "react-pure-render": "^1.0.2",
